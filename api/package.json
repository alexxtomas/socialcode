    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.83",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.49.0",
    "nodemon": "^3.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "express": "^4.18.2"
  }
